
namespace rpc {

enum MsgProtocolType
{
    MPT_Bin,
    MPT_Json,
};

struct RpcHeader {
    uint8            version;
    MsgProtocolType  msgType;
    string           serviceName;
    uint16           bodyLen;
};

struct RequestMsg {
	int64 msgSeqId;
	uint16 msgId;
	array<int8>  buf;
};

struct ResponseMsg {
	int64 msgSeqId;
	uint16 msgId;
	array<int8>  buf;
};
}